# Disguise as a Chrome browser
user-agent = "Mozilla/5.0 (Macintosh; U; Intel Mac OS X 7_2_7; en-US) AppleWebKit/602.13 (KHTML, like Gecko) Chrome/48.0.1381.271 Safari/601"

# When following a redirect, automatically set the previous URL as referer.
referer = ";auto"

# Wait 60 seconds before timing out.
connect-timeout = 60
